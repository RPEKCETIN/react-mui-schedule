{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    "react-hooks/rules-of-hooks":"off",
    "quotes":"off",
    "no-unused-vars":"warn",
    "max-len":"warn",
    "react/jsx-key":"off",
    "linebreak-style":"off",
        "no-tabs":"off",
        "no-mixed-spaces-and-tabs":"off",
        "react/react-in-jsx-scope": "off",
        "semi":["error","never"],
        "object-curly-spacing":["error","always"],
        "comma-dangle":["error","never"],
        "arrow-spacing": ["error", { "before": true, "after": true }],
        "space-infix-ops":"off",
        "react/display-name": "off"
    }
}
